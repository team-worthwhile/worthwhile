// calculate the sum from 1 to n
function Integer gauss_sum (Integer n) {
	
	if (n < 1)	 {
		return 1 - 2
	} else {
		Integer sum := 0
		Integer i := n
		while (i > 0) {
			sum := sum + i
			i := i - 1
		}
		return sum
