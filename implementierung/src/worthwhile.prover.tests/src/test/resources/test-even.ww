// EXPECTED:VALID
function Boolean even(Integer x)
_ensures (exists Integer k1 : 2 * k1 = x && _return = true) || (exists Integer k2 : 2 * k2 - 1 = x && _return = false)
{
    Integer temp := x % 2
    if temp = 0 {
        return true
    }
    if temp = 1 {
        return false
    }
    return false
}
