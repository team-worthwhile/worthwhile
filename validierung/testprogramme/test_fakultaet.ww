//iterative calculation of the faculty of fac 

function Integer faculty(Integer fac)
{
	Integer current := fac
	
	if (current < 0)
    {
	    return -1
    }
	else
    {
	    Integer res := 1
	    while (current > 0)
        {
	       res := res * fac
	       current := current - 1				
        }
	return res
	}
}
