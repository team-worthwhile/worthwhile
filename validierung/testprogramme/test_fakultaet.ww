//iterative calculation of the faculty of fac 

function Integer faculty(Integer fac)
{
	if (fac < 0)
        {
	    return -1
        }
	if (fac = 0)
        {
	    return 1
	}
	else
        {
	    Integer res := 1
	    while (fac > 0)
            {
	       res := res * fac
	       fac := fac - 1				
            }
	    return res
       }
}
