//iterative calculation of the faculty of fac 

function faculty(fac : Integer) : Integer
{
	if (fac < 0)
        {
	    return -1
        }
	else if (fac = 0)
        {
	    return 1
	}
	else
        {
	    res : Integer := 1
	    while (fac > 0)
            {
	       res := res * fac
	       fac := fac - 1				
            }
	    return res
       }
}
